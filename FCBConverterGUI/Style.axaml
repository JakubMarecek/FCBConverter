						<Border BorderBrush="Black" BorderThickness="2" Background="LightGray" CornerRadius="5" />
						<TextBlock Name="btntext" Text="{TemplateBinding Content}" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="Black" FontFamily="{StaticResource MainFont}">
						</TextBlock>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

